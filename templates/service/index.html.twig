                                        <span class="text-muted">cost: {{ subscription.price / 100}} {{ subscription.currency }}  {% if subscription.currency != 'UAH' %}(<span>{{  subscription.price/100 * rates[subscription.currency] | round}} UAH</span>){% endif %}</span>
                                    </div>
